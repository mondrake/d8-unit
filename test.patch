+
+      case 'change':
+        return new DiffOpChange($source, $target);
+
+      case Differ::ADDED:
+        return new DiffOpAdd($source);
+
+      case Differ::REMOVED:
+        return new DiffOpDelete($source);
+
+    }
+  }
